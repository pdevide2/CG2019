Imports System.DataImports System.Collections.GenericImports DALImports System.Data.CommonImports System.Data.SqlClientPublic Class Movimento_equipamento_quarentenaBLLPublic Enum flagOp        Limpar = 0        Novo = 1        Alterar = 2        Excluir = 3        Consulta = 4        Leitura = 5    End Enum    Public Sub GravarBLL(ByVal acao As Integer, ByVal oMovimento_equipamento_quarentena As DTO.Cg_movimento_equipamento_quarentena)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_ROMANEIO",DbType.Int32,oMovimento_equipamento_quarentena.Id_romaneio))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DATA_MOVTO",DbType.Datetime,oMovimento_equipamento_quarentena.Data_movto))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_RESPONSAVEL",DbType.Int32,oMovimento_equipamento_quarentena.Id_responsavel))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_LOJA_ORIGEM",DbType.Int32,oMovimento_equipamento_quarentena.Id_loja_origem))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_LOJA_DESTINO",DbType.Int32,oMovimento_equipamento_quarentena.Id_loja_destino))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@TIPO_OP",DbType.String,oMovimento_equipamento_quarentena.Tipo_op))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@USER_INS",DbType.Int32,oMovimento_equipamento_quarentena.User_ins))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DATA_INS",DbType.Datetime,oMovimento_equipamento_quarentena.Data_ins))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@USER_UPD",DbType.Int32,oMovimento_equipamento_quarentena.User_upd))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DATA_UPD",DbType.Datetime,oMovimento_equipamento_quarentena.Data_upd))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@QTD_TOTAL",DbType.Int32,oMovimento_equipamento_quarentena.Qtd_total))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@QTD_RECEBIDA",DbType.Int32,oMovimento_equipamento_quarentena.Qtd_recebida))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ROMANEIO_ESTOQUE",DbType.Int32,oMovimento_equipamento_quarentena.Romaneio_estoque))        Try            Dim cmd As String            cmd = Nothing            If acao = flagOp.Alterar Then                cmd = "spUpd_Cg_movimento_equipamento_quarentena"            ElseIf acao = flagOp.Novo Then                cmd = "spIns_Cg_movimento_equipamento_quarentena"            End If            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Sub ExcluirBLL(_KeyId As Integer)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_ROMANEIO",DbType.Int32,_KeyId))        Try            Dim cmd As String            cmd = Nothing            cmd = "spDel_Cg_movimento_equipamento_quarentena"            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Function PesquisaPorIdModelBLL(ByVal KeyId As Integer, ByVal strTabela As String, colunaId As String) As DTO.Cg_movimento_equipamento_quarentena        Dim sql = "SELECT * FROM " & strTabela & " WHERE " & colunaId & " = " & KeyId.ToString        Dim dr As SqlDataReader        dr = DAL.DALGenerico.ExecutarComando("SQLServer", sql, CommandType.Text, Nothing, DALGenerico.TipoDeComando.ExecuteReader)        Dim oModelo As New DTO.Cg_movimento_equipamento_quarentena        If dr.HasRows Then            dr.Read()        oModelo.Id_romaneio = CInt(dr.Item("Id_romaneio"))        oModelo.Data_movto = CDate(dr.Item("Data_movto"))        oModelo.Id_responsavel = CInt(dr.Item("Id_responsavel"))        oModelo.Id_loja_origem = CInt(dr.Item("Id_loja_origem"))        oModelo.Id_loja_destino = CInt(dr.Item("Id_loja_destino"))        oModelo.Tipo_op = dr.Item("Tipo_op")        oModelo.User_ins = CInt(dr.Item("User_ins"))        oModelo.Data_ins = CDate(dr.Item("Data_ins"))        oModelo.User_upd = CInt(dr.Item("User_upd"))        oModelo.Data_upd = CDate(dr.Item("Data_upd"))        oModelo.Qtd_total = CInt(dr.Item("Qtd_total"))        oModelo.Qtd_recebida = CInt(dr.Item("Qtd_recebida"))        oModelo.Romaneio_estoque = CInt(dr.Item("Romaneio_estoque"))        Else        oModelo.Id_romaneio = -1        End If        Return oModelo    End FunctionEnd Class