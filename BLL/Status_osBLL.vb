'********************************************************'* Status_osBLL'* Classe BLL do cadastro de Status da OS'* PAULO EDUARDO DEVIDE'* 30-JUL-2016'*'* Métodos:'* GravarBLL()'* ExcluirBLL()'* PesquisaPorIdModelBLL()'*' ********************************************************Imports System.DataImports System.Collections.GenericImports DALImports System.Data.CommonImports System.Data.SqlClientPublic Class Status_osBLL    Public Enum flagOp        Limpar = 0        Novo = 1        Alterar = 2        Excluir = 3        Consulta = 4        Leitura = 5    End Enum    Public Sub GravarBLL(ByVal acao As Integer, ByVal oStatus_os As DTO.Cg_status_os)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_STATUS", DbType.Int32, oStatus_os.Id_status))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DESC_STATUS", DbType.String, oStatus_os.Desc_status))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@USER_INS", DbType.Int32, oStatus_os.User_ins))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DATA_INS", DbType.DateTime, oStatus_os.Data_ins))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@USER_UPD", DbType.Int32, oStatus_os.User_upd))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@DATA_UPD", DbType.DateTime, oStatus_os.Data_upd))        Try            Dim cmd As String            cmd = Nothing            If acao = flagOp.Alterar Then                cmd = "spUpd_Cg_status_os"            ElseIf acao = flagOp.Novo Then                cmd = "spIns_Cg_status_os"            End If            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Sub ExcluirBLL(_KeyId As Integer)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_STATUS", DbType.Int32, _KeyId))        Try            Dim cmd As String            cmd = Nothing            cmd = "spDel_Cg_status_os"            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Function PesquisaPorIdModelBLL(ByVal KeyId As Integer, ByVal strTabela As String, colunaId As String) As DTO.Cg_status_os        Dim sql = "SELECT * FROM " & strTabela & " WHERE " & colunaId & " = " & KeyId.ToString        Dim dr As SqlDataReader        dr = DAL.DALGenerico.ExecutarComando("SQLServer", sql, CommandType.Text, Nothing, DALGenerico.TipoDeComando.ExecuteReader)        Dim oModelo As New DTO.Cg_status_os        If dr.HasRows Then            dr.Read()            oModelo.Id_status = CInt(dr.Item("Id_status"))            oModelo.Desc_status = dr.Item("Desc_status")            oModelo.User_ins = CInt(dr.Item("User_ins"))            oModelo.Data_ins = CDate(dr.Item("Data_ins"))            oModelo.User_upd = CInt(dr.Item("User_upd"))            oModelo.Data_upd = CDate(dr.Item("Data_upd"))        Else            oModelo.Id_status = -1        End If        Return oModelo    End FunctionEnd Class