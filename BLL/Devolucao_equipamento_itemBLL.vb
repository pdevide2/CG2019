'****************************************************************'* Devolucao_equipamento_itemBLL'* Classe BLL do cadastro de devolucao dos itens de equipamentos'* PAULO EDUARDO DEVIDE'* 30-JUL-2016'*'* Métodos:'* GravarBLL()'* ExcluirBLL()'* PesquisaPorIdModelBLL()'*' ****************************************************************Imports System.DataImports System.Collections.GenericImports DALImports System.Data.CommonImports System.Data.SqlClientPublic Class Devolucao_equipamento_itemBLL    Public Enum flagOp        Limpar = 0        Novo = 1        Alterar = 2        Excluir = 3        Consulta = 4        Leitura = 5    End Enum    Public Sub GravarBLL(ByVal acao As Integer, ByVal oDevolucao_equipamento_item As DTO.Cg_devolucao_equipamento_item)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_ROMANEIO", DbType.Int32, oDevolucao_equipamento_item.Id_romaneio))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@UNIQUE_KEYID", DbType.String, oDevolucao_equipamento_item.Unique_keyid))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_EQUIPAMENTO", DbType.Int32, oDevolucao_equipamento_item.Id_equipamento))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@SERIE", DbType.String, oDevolucao_equipamento_item.Serie))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@QTD", DbType.Int32, oDevolucao_equipamento_item.Qtd))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@VALOR", DbType.Decimal, oDevolucao_equipamento_item.Valor))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_MOTIVO", DbType.Int32, oDevolucao_equipamento_item.Id_motivo))        Try            Dim cmd As String            cmd = Nothing            If acao = flagOp.Alterar Then                cmd = "spUpd_Cg_devolucao_equipamento_item"            ElseIf acao = flagOp.Novo Then                cmd = "spIns_Cg_devolucao_equipamento_item"            End If            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Sub ExcluirBLL(_KeyId As Integer, _UniqueId As String)        Dim listaParametros As New List(Of DbParameter)        listaParametros.Add(DAL.DALGenerico.CriarParametro("@ID_ROMANEIO", DbType.Int32, _KeyId))        listaParametros.Add(DAL.DALGenerico.CriarParametro("@UNIQUE_KEYID", DbType.String, _UniqueId))        Try            Dim cmd As String            cmd = Nothing            cmd = "spDel_Cg_devolucao_equipamento_item"            DAL.DALGenerico.ExecutarComando("SQLServer", cmd, CommandType.StoredProcedure, listaParametros, DAL.DALGenerico.TipoDeComando.ExecuteNonQuery)        Catch ex As Exception            Throw ex        End Try    End Sub    Public Function PesquisaPorIdModelBLL(ByVal KeyId As Integer, ByVal strTabela As String, colunaId As String) As DTO.Cg_devolucao_equipamento_item        Dim sql = "SELECT * FROM " & strTabela & " WHERE " & colunaId & " = " & KeyId.ToString        Dim dr As SqlDataReader        dr = DAL.DALGenerico.ExecutarComando("SQLServer", sql, CommandType.Text, Nothing, DALGenerico.TipoDeComando.ExecuteReader)        Dim oModelo As New DTO.Cg_devolucao_equipamento_item        If dr.HasRows Then            dr.Read()            oModelo.Id_romaneio = CInt(dr.Item("Id_romaneio"))            oModelo.Unique_keyid = dr.Item("Unique_keyid")            oModelo.Id_equipamento = CInt(dr.Item("Id_equipamento"))            oModelo.Serie = dr.Item("Serie")            oModelo.Qtd = CInt(dr.Item("Qtd"))            oModelo.Valor = CDec(dr.Item("Valor"))            oModelo.Id_motivo = CInt(dr.Item("Id_motivo"))        Else            oModelo.Id_romaneio = -1            oModelo.Unique_keyid = -1        End If        Return oModelo    End FunctionEnd Class