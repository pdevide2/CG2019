CREATE PROCEDURE spUpd_Cg_movimento_equipamento	@ID_ROMANEIO As int,	@DATA_MOVTO As datetime,	@ID_RESPONSAVEL As int,	@ID_LOJA_ORIGEM As int,	@ID_LOJA_DESTINO As int,	@TIPO_OP As char(1),	@USER_INS As int = NULL,	@DATA_INS As datetime = NULL,	@USER_UPD As int = NULL,	@DATA_UPD As datetime = NULLASBEGINUPDATE CG_MOVIMENTO_EQUIPAMENTO SET	data_movto = @data_movto,	id_responsavel = @id_responsavel,	id_loja_origem = @id_loja_origem,	id_loja_destino = @id_loja_destino,	tipo_op = @tipo_op,	user_ins = @user_ins,	data_ins = @data_ins,	user_upd = @user_upd,	data_upd = @data_updWHERE	ID_ROMANEIO = @ID_ROMANEIOEND------------------------------------------------------------